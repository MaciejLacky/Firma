@model Firma.Data.Data.CMS.Aktualnosc
@{
    ViewData["Title"] = @Model.Tytul;
}
<div class="container">
    <div class="row">
        <div class="col s10">
            <div class="text-capitalize">Aktualność</div>
            <div class="text-accent-2">Tytuł aktualności: @Model.Tytul </div>
            <div class="text-accent-3">@Model.Tekst</div>
            <br />
            <div class="text-center">Data i godzina edycji: @DateTime.Now.ToString()</div>
        </div>             
    </div>

</div>



